<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>aa</display-name>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>companyServlet</servlet-name>
  	<servlet-class>com.ctf.web.CompanyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>companyServlet</servlet-name>
  	<url-pattern>/company</url-pattern>
  </servlet-mapping>
  
    <servlet>
  	<servlet-name>customerServlet</servlet-name>
  	<servlet-class>com.ctf.web.CustomerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>customerServlet</servlet-name>
  	<url-pattern>/customer</url-pattern>
  </servlet-mapping>
  
  
    <servlet>
  	<servlet-name>userServlet</servlet-name>
  	<servlet-class>com.ctf.web.UserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>userServlet</servlet-name>
  	<url-pattern>/user</url-pattern>
  </servlet-mapping>
  
</web-app>